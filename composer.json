{
  "name": "choros-microservices/skeleton",
  "description": "Skeleton of base micro-service app",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Krzysztof Chlista≈Ça",
      "email": "k.chlistala@10ka.pl"
    }
  ],
  "require": {
    "symfony/symfony": "3.0.*",
    "symfony/monolog-bundle": "^2.8",
    "sensio/framework-extra-bundle": "3.0.*",
    "sensio/distribution-bundle": "~5.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "friendsofsymfony/rest-bundle": "^1.7",
    "nelmio/api-doc-bundle": "^2.12",
    "prooph/service-bus": "~5.0",
    "container-interop/container-interop" : "^1.1",
    "proophsoftware/prooph-interop-bundle": "^0.1",
    "sandrokeil/interop-config": "^1.0",
    "league/flysystem": "^1.0",
    "oneup/flysystem-bundle": "^1.1",
    "doctrine/dbal": "v2.5.4"
  },
  "require-dev": {
    "sensio/generator-bundle": "^3.0",
    "phpunit/phpunit": "5.1.4"
  },
  "scripts": {
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ]
  },
  "autoload": {
    "psr-4": { "": "src/" },
    "classmap": [ "symfony/MicroKernel.php" ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "symfony",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "public",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "symfony/config/parameters.yml"
    }
  }
}
